/* SIMULATE LOOP WITH WILE */
i = 10;
WHILE i DO
    //do something
    i = i - 1;
END;

/* ADD TWO NUMBERS */
x_1 = 8;
x_2 = 9;
LOOP x_1 DO
    x_2 = x_2 + 1;
END;

/* MULTIPLY TWO NUMBERS */
x_1 = 7;
x_2 = 3;
result = 0;
LOOP x_1 DO // outer loop running x_1 times
    LOOP x_2 DO // inner loop adding x_2 to result
        result = result + 1;
    END;
END;